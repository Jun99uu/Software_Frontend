        <activity
            android:name=".MakePortfolioActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name=".SettingActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name=".PlanCollectionDetailActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name=".PortfolioActivity"
            android:screenOrientation="portrait"
